--  Node definitions

        -- 1.3.6.1.4.1.2011.6.187.1
        hwDocsQamVideoGlobalConfigObject   OBJECT IDENTIFIER ::= { hwEqamAn 1 }
        
        -- 1.3.6.1.4.1.2011.6.187.1.1
        hwDocsQamVideoTimeOutInterval  OBJECT-TYPE
            SYNTAX      Integer32 (1..6000)
            UNITS       "second"
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the video timeout interval.
                 Default: 60
                "
            ::= { hwDocsQamVideoGlobalConfigObject 1 }

        -- 1.3.6.1.4.1.2011.6.187.1.2
        hwDocsQamAutoConfigIppoolAddressType  OBJECT-TYPE
            SYNTAX      InetAddressType
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the address type of the address pool. Only IPv4 addresses are supported.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 2 }
                  
        -- 1.3.6.1.4.1.2011.6.187.1.3
        hwDocsQamAutoConfigIppoolStartAddress  OBJECT-TYPE
            SYNTAX      InetAddress
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the start address of the address pool. 
                 To delete an IP address pool, bind hwDocsQamAutoConfigIppoolStartAddress and hwDocsQamAutoConfigIppoolEndAddress together and set their values to 0xff 0xff 0xff 0xff.
                 The value of hwDocsQamAutoConfigIppoolStartAddress or hwDocsQamAutoConfigIppoolEndAddress must be set to a valid unicast IP address in other scenarios.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 3 }
      
        -- 1.3.6.1.4.1.2011.6.187.1.4
        hwDocsQamAutoConfigIppoolEndAddress  OBJECT-TYPE
            SYNTAX      InetAddress
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the end address of the address pool.
                 To delete an IP address pool, bind hwDocsQamAutoConfigIppoolStartAddress and hwDocsQamAutoConfigIppoolEndAddress together and set their values to 0xff 0xff 0xff 0xff.
                 The value of hwDocsQamAutoConfigIppoolStartAddress or hwDocsQamAutoConfigIppoolEndAddress must be set to a valid unicast IP address in other scenarios.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 4 }
      
        -- 1.3.6.1.4.1.2011.6.187.1.5
        hwDocsQamAutoConfigIppoolVlanId  OBJECT-TYPE
            SYNTAX      Integer32 (-1|1..4093)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the VLAN ID of the video domain. The default value is -1, indicating that the VLAN ID is invalid.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 5 }
      
        -- 1.3.6.1.4.1.2011.6.187.1.6
        hwDocsQamAutoConfigVideoProfile  OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..15))
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Automatically pre-configures the name of the video profile.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 6 }
            
        -- 1.3.6.1.4.1.2011.6.187.1.8
        hwDocsQamMulticastVlanId  OBJECT-TYPE
            SYNTAX      Integer32 (-1|1..4093)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the multicast VLAN ID of the video service. The default value is -1, indicating that the multicast VLAN ID is invalid.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 8 }
            
        -- 1.3.6.1.4.1.2011.6.187.1.9
        hwDocsQamSecondaryMulticastVlanId  OBJECT-TYPE
            SYNTAX      Integer32 (-1|1..4093)
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the secondary multicast VLAN ID. The default value is -1, indicating that the multicast VLAN ID is invalid.
                "
            ::= { hwDocsQamVideoGlobalConfigObject 9 }
            
        -- 1.3.6.1.4.1.2011.6.187.1.10
        hwDocsQamGratuitousArpPeriodSendMode  OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  enabled(1),
                  disabled(2)
                  }
            MAX-ACCESS  read-write
            STATUS      current
            DESCRIPTION
                "Indicates the gratuitous ARP period send mode, the default value is disabled(2).
                 Options: 
                 1. enabled(1)      -indicates the gratuitous ARP period send mode is enabled
                 2. disabled(2)     -indicates the gratuitous ARP period send mode is disabled
                "
            ::= { hwDocsQamVideoGlobalConfigObject 10 }            
    
        -- 1.3.6.1.4.1.2011.6.187.2
        hwDocsQamVideoProfileTable    OBJECT-TYPE
            SYNTAX          SEQUENCE OF HwDocsQamVideoProfileEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the video profile table used to manage information including video mapping.
                 The index of this table is hwDocsQamVideoProfileName.
                "
            ::= { hwEqamAn 2 }
    
        -- 1.3.6.1.4.1.2011.6.187.2.1
        hwDocsQamVideoProfileEntry    OBJECT-TYPE
            SYNTAX          HwDocsQamVideoProfileEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the video profile table used to manage information including video mapping.
                 The index of this entry is hwDocsQamVideoProfileName.
                "
            INDEX   
                { 
                hwDocsQamVideoProfileName
                }
            ::= { hwDocsQamVideoProfileTable  1 }
        
        HwDocsQamVideoProfileEntry ::=
            SEQUENCE 
                { 
                hwDocsQamVideoProfileName
                  OCTET STRING,
                hwDocsQamVideoProfileRowStatus
                  RowStatus   
                }
        
        -- 1.3.6.1.4.1.2011.6.187.2.1.1   
        hwDocsQamVideoProfileName        OBJECT-TYPE
            SYNTAX       OCTET STRING (SIZE (1..15))
            MAX-ACCESS   not-accessible
            STATUS       current
            DESCRIPTION
              "Indicates the video profile name. 
              "
            ::= { hwDocsQamVideoProfileEntry 1}
          
        -- 1.3.6.1.4.1.2011.6.187.2.1.2 
        hwDocsQamVideoProfileRowStatus        OBJECT-TYPE
            SYNTAX       RowStatus
            MAX-ACCESS   read-create
            STATUS       current
            DESCRIPTION
                "Row status, used to query, create, modify, and delete a video profile table. 
                 It supports the active(1), createAndGo(4), and destroy(6) operations.
                 During query, the value of this leaf is always active(1).
                 During deletion, specify the value of this leaf to destroy(6).
                 During creation, specify the value of this leaf to createAndGo(4).
                 To modify the parameters of a video profile table, this leaf need not be issued together.
                 Options:
                 1. active(1)      -indicates the query operation
                 2. createAndGo(4) -indicates the creating operation
                 3. destroy(6)     -indicates the deleting operation
                "
            ::= { hwDocsQamVideoProfileEntry 2}
          
        -- 1.3.6.1.4.1.2011.6.187.3
        hwDocsQamVideoMapTable    OBJECT-TYPE
            SYNTAX          SEQUENCE OF HwDocsQamVideoMapEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the video mapping table used to configure the mapping between video streams and cable channels.
                 The indexes of this table are hwDocsQamVideoProfileName and hwDocsQamVideoMapIndex.
                "
            ::= { hwEqamAn 3 }
    
        -- 1.3.6.1.4.1.2011.6.187.3.1
        hwDocsQamVideoMapEntry    OBJECT-TYPE
            SYNTAX          HwDocsQamVideoMapEntry
            MAX-ACCESS      not-accessible
            STATUS          current
            DESCRIPTION
                "Indicates the video mapping table used to configure the mapping between video streams and cable channels.
                 The indexes of this entry are hwDocsQamVideoProfileName and hwDocsQamVideoMapIndex.
                "
            INDEX   
                { 
                hwDocsQamVideoProfileName,
                hwDocsQamVideoMapIndex
                }
            ::= { hwDocsQamVideoMapTable  1 }
        
        HwDocsQamVideoMapEntry ::=
            SEQUENCE 
                { 
                hwDocsQamVideoMapIndex
                  Unsigned32,
                hwDocsQamVideoMapUdpPort
                  InetPortNumber,
                hwDocsQamVideoMapStreamType
                  INTEGER,
                hwDocsQamVideoMapInputProgram
                  Integer32,
                hwDocsQamVideoMapOutputProgram
                  Integer32,
                hwDocsQamVideoMapJitter
                  Integer32,
                hwDocsQamVideoMapPmtPid
                  Integer32,
                hwDocsQamVideoMapChannelList
                  ChannelList,
                hwDocsQamVideoMapRowStatus
                  RowStatus,
                hwDocsQamVideoMapMode
                  INTEGER,
                hwDocsQamVideoMapNITRemap
                  Integer32,
                hwDocsQamVideoMapFilterPIDList
                  OCTET STRING,
                hwDocsQamVideoMapInputTsIndex
                  Integer32
                }
        
        -- 1.3.6.1.4.1.2011.6.187.3.1.1   
        hwDocsQamVideoMapIndex        OBJECT-TYPE
            SYNTAX      Unsigned32 (1..65535|4294967295)
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "Indicates the index of a video mapping table. 
                 You can also fill 4294967295 and the system will automatically allocate an index.
                "
            ::= { hwDocsQamVideoMapEntry 1 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.2   
        hwDocsQamVideoMapUdpPort        OBJECT-TYPE
            SYNTAX      InetPortNumber
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the UDP port number. The default value is 0, indicating that the UDP port number is invalid.
                 Range: 0-65535
                "
            ::= { hwDocsQamVideoMapEntry 2 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.3   
        hwDocsQamVideoMapStreamType        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  data(1),
                  spts(2),
                  mpts(3),
                  plant(4)
                  }
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the program stream type.
                 Options: 
                 1. data(1)   -indicates the data type
                 2. spts(2)   -indicates the spts type
                 3. mpts(3)   -indicates the mpts type
                 4. plant(4)  -indicates the plant type
                "
            ::= { hwDocsQamVideoMapEntry 3 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.4   
        hwDocsQamVideoMapInputProgram        OBJECT-TYPE
            SYNTAX      Integer32 (-1|1..65535)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the program ID of the input stream. The default value is -1, indicating that the program ID is ignored.
                "
            ::= { hwDocsQamVideoMapEntry 4 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.5   
        hwDocsQamVideoMapOutputProgram        OBJECT-TYPE
            SYNTAX      Integer32 (-1|1..65535)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the program ID of the output stream. The default value is -1, indicating that the program ID is ignored.
                "
            ::= { hwDocsQamVideoMapEntry 5 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.6   
        hwDocsQamVideoMapJitter        OBJECT-TYPE
            SYNTAX      Integer32 (-1|0..250)
            UNITS       "millisecond"
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the network jitter allowed. 
                 If hwDocsQamVideoMapStreamType is data, the default value is -1, indicating that network jitter is ignored.
                 If hwDocsQamVideoMapStreamType is spts or mpts, the default value is 100.
                "
            ::= { hwDocsQamVideoMapEntry 6 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.7   
        hwDocsQamVideoMapPmtPid        OBJECT-TYPE
            SYNTAX      Integer32 (-1|32..8190)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the PMT PID of the output program.
                 If hwDocsQamVideoMapStreamType is data or mpts, the default value is -1, indicating that output programs' PMT PIDs are ignored.
                "
            ::= { hwDocsQamVideoMapEntry 7 }
          
        -- 1.3.6.1.4.1.2011.6.187.3.1.8   
        hwDocsQamVideoMapChannelList        OBJECT-TYPE
            SYNTAX      ChannelList
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the channel list for output videos.
                "
            ::= { hwDocsQamVideoMapEntry 8 }
            
        -- 1.3.6.1.4.1.2011.6.187.3.1.9
        hwDocsQamVideoMapRowStatus        OBJECT-TYPE
            SYNTAX       RowStatus
            MAX-ACCESS   read-create
            STATUS       current
            DESCRIPTION
                "Row status, used to query, create, modify, and delete a video mapping table. 
                 It supports the active(1), createAndGo(4), and destroy(6) operations.
                 During query, the value of this leaf is always active(1).
                 During deletion, specify the value of this leaf to destroy(6).
                 During creation, specify the value of this leaf to createAndGo(4).
                 To modify the parameters of a video mapping table, this leaf need not be issued together.
                 Options:
                 1. active(1)      -indicates the query operation
                 2. createAndGo(4) -indicates the creating operation
                 3. destroy(6)     -indicates the deleting operation
                "
            ::= { hwDocsQamVideoMapEntry 9}
            
        -- 1.3.6.1.4.1.2011.6.187.3.1.12  
        hwDocsQamVideoMapMode        OBJECT-TYPE
            SYNTAX      INTEGER
                  {
                  passthrough(1),
                  invalid(255)
                  }
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the video stream map mode, the default value is invalid(255).
                 Options: 
                 1. passthrough(1)   -indicates the MPTS video stream mode is passthrough
                 2. invalid(255)     -indicates the video stream mode is ignored if hwDocsQamVideoMapStreamType is not mpts
                "
            ::= { hwDocsQamVideoMapEntry 12 }
            
        -- 1.3.6.1.4.1.2011.6.187.3.1.13   
        hwDocsQamVideoMapNITRemap        OBJECT-TYPE
            SYNTAX      Integer32 (-1 | 16 | 32..8190)
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the output NIT PID which the input NIT PID mapped to. 
                 If hwDocsQamVideoMapStreamType is not mpts, the default value is always -1, indicating that the NIT remap PID is ignored.
                 If hwDocsQamVideoMapStreamType is mpts, vlaue -1 means cancel the NIT remap.
                "
            ::= { hwDocsQamVideoMapEntry 13 }
            
        -- 1.3.6.1.4.1.2011.6.187.3.1.14   
        hwDocsQamVideoMapFilterPIDList        OBJECT-TYPE
            SYNTAX      OCTET STRING (SIZE (0..64))
            MAX-ACCESS  read-create
            STATUS      current
            DESCRIPTION
                "Indicates the filter PID list which are not allowed to forward.
                 Filter PID consists of two bytes and no greater than 8190. 
                 Max of 32 filter PIDs can be configured for each video mapping.
                 If hwDocsQamVideoMapStreamType is not mpts, the default value is always null string, indicating that the filter PID is ignored. 
                 If hwDocsQamVideoMapStreamType is mpts, the default value null string means cancel the PID filter.
                "
            ::= { hwDocsQamVideoMapEntry 14 }

        -- 1.3.6.1.4.1.2011.6.187.3.1.15
        hwDocsQamVideoMapInputTsIndex   OBJECT-TYPE
            SYNTAX       Integer32 (-1|1..65535)
            MAX-ACCESS   read-create
            STATUS       current
            DESCRIPTION
                "Indicates the index of the transport stream, the default value is -1, indicating that the index of the transport stream is ignored.
                "
            ::= { hwDocsQamVideoMapEntry 15}
  
  
        -- 1.3.6.1.4.1.2011.6.187.4
