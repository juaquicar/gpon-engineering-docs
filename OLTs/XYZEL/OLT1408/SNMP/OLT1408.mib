        ::= { policyEntry 20 }

        policyMirrorPort OBJECT-TYPE
        SYNTAX  INTEGER
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
        	"Mirror Port."
        ::= { policyEntry 21 }

        policyCounter OBJECT-TYPE
        SYNTAX  EnabledStatus
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Counter enabled/disabled for policy."
        ::= { policyEntry 22 }

        policyLogging OBJECT-TYPE
        SYNTAX  EnabledStatus
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
            "Logging enabled/disabled for policy."
        ::= { policyEntry 23 }

        policyRuleSet OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-write
        STATUS  current
        DESCRIPTION
                      	"policy Rule Set."
        ::= { policySetup 2 }

	policyClearCounterAll OBJECT-TYPE
	SYNTAX  EnabledStatus
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
			"Clear policy all counter, always return disable"
	::= { policySetup 3 }

	policyClearTable OBJECT-TYPE
