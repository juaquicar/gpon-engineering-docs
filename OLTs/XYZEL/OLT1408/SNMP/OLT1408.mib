--  ontLanSetup Table
    ontLanSetupTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF OntLanSetupEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        ""
    ::= { ontSetup 9 }

    ontLanSetupEntry OBJECT-TYPE
    SYNTAX      OntLanSetupEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "An entry in ontLanSetupTable"
    INDEX {
        dot1dBasePort,
        ontNumber,
        ontLanId
    }
    ::= { ontLanSetupTable 1 }

    OntLanSetupEntry ::=
    SEQUENCE {
        ontLanId                     INTEGER,
        ontLanStaticIpAddr           IpAddress,
        ontLanStaticIpMask           IpAddress,
        ontLanDHCPMode               INTEGER,
        ontLanDHCPRelayServerIP      IpAddress,
        ontLanDHCPServerPoolBegin    IpAddress,
        ontLanDHCPServerPoolEnd      IpAddress,
        ontLanSetupRowStatus         RowStatus,
        ontLanSetupDnsType           INTEGER,
        ontLanSetupPrimaryDns        IpAddress,
        ontLanSetupSecondaryDns      IpAddress
    }

    ontLanId OBJECT-TYPE
    SYNTAX  INTEGER (0..8)
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "ONT LAN Setup Instance ID in 0-8"
    ::= { ontLanSetupEntry 1 }

	ontLanStaticIpAddr OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup Static IP Address.
		    - ip address in 1.0.0.1-126.255.255.254, 128.0.0.1-223.255.255.254
		    - ip address must be subnet address
		    - ip address must not be subnet broadcast address
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 2 }

	ontLanStaticIpMask OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup Static IP Mask.
		    - ip mask in 128.0.0.0-255.255.255.254
		    - ip mask must be contiguous 1 bits format
		    - ip subnet must be unique in this ont
		    - if dhcp server pool exist
		   	- dhcp server pool must be ip subnet
		        - dhcp server pool must not be static ip subnet broadcast address
		        - dhcp server pool must not be static ip address
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 3 }

    ontLanDHCPMode OBJECT-TYPE
    SYNTAX  INTEGER{
           disable(0),
           server(1),
		   relay(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "ONT LAN Setup DHCP Mode, 
		    disable:
		        DHCP Relay Server IP: must 0.0.0.0
		        DHCP Server Pool Begin: must 0.0.0.0
		        DHCP Server Pool End: must 0.0.0.0
			server:
		        DHCP Relay Server IP: must 0.0.0.0
		        DHCP Server Pool Begin: mandatory
		        DHCP Server Pool End: mandatory
			relay:
		        DHCP Relay Server IP: mandatory
		        DHCP Server Pool Begin: must 0.0.0.0
		        DHCP Server Pool End: must 0.0.0.0			
		"
    ::= { ontLanSetupEntry 4 }

	ontLanDHCPRelayServerIP OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup DHCP Relay Server IP Address.
		    - if relay server ip is not 0.0.0.0
			    - DHCP mode must be relay
				- DHCP Server Pool Begin: must 0.0.0.0
				- DHCP Server Pool End: must 0.0.0.0
		    - relay server ip in 0.0.0.1-223.255.255.255
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 5 }

	ontLanDHCPServerPoolBegin OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup DHCP Server IP Pool Begin IP Address.
		    - if pool begin ip is not 0.0.0.0
			    - DHCP mode must be server
				- DHCP Relay Server IP: must 0.0.0.0
		    - pool begin ip in in 1.0.0.1-126.255.255.254, 128.0.0.1-223.255.255.254
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 6 }

	ontLanDHCPServerPoolEnd OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup DHCP Server IP Pool End IP Address.
		    - dhcp server pool must be unique in this ont
		    - if pool end ip is not 0.0.0.0
			    - DHCP mode must be server
				- DHCP Relay Server IP: must 0.0.0.0
		    - pool end ip in in 1.0.0.1-126.255.255.254, 128.0.0.1-223.255.255.254
		    - pool end ip must greater than pool begin ip
		    - if static ip exist
		      - dhcp server pool must be static ip subnet
		      - dhcp server pool must not be static ip subnet broadcast address
		      - dhcp server pool must not be static ip address
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 7 }

    ontLanSetupRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "Row status for ONT Lan Setup."
    ::= { ontLanSetupEntry 8 }
	
    ontLanSetupDnsType OBJECT-TYPE
    SYNTAX  INTEGER{
           proxy(0),
           static(1),
		   isp(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "ONT LAN Setup DNS Type. 
		    - proxy:
		        Primary Dns: must 0.0.0.0
		        Secondary Dns: must 0.0.0.0
			- static:
		        Primary Dns is mandatory
		        Secondary Dns is optional
			- isp:
		        Primary Dns: must 0.0.0.0
		        Secondary Dns: must 0.0.0.0
            - default: proxy            
		"
    ::= { ontLanSetupEntry 9 }

	ontLanSetupPrimaryDns OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup primary DNS server.
		    - DNS ip address in 0.0.0.1-223.255.255.255
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 10 }

	ontLanSetupSecondaryDns OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"ONT LAN Setup secondary DNS server.
		    - DNS ip address in 0.0.0.1-223.255.255.255
		    - default: 0.0.0.0
		"
	::= { ontLanSetupEntry 11 }
	
