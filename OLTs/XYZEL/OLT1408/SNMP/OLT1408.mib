	cdrIgmp OBJECT IDENTIFIER ::= { cdr 1 }

	cdrIgmpSetup OBJECT IDENTIFIER ::= { cdrIgmp 1 }

	cdrIgmpActive OBJECT-TYPE
		SYNTAX INTEGER {
			disable(0),
			enable(1)
		}
		MAX-ACCESS  read-write
		STATUS  current
		DESCRIPTION
			"Enables or disables CDR(Call Detail Record) for IGMP."
		::= { cdrIgmpSetup 1 }

	cdrIgmpLogSize OBJECT-TYPE
		SYNTAX INTEGER
		MAX-ACCESS read-write
        STATUS current
        DESCRIPTION
			"Sets the maximum number of logs of CDR IGMP."
		::= { cdrIgmpSetup 2 }

	cdrIgmpStatus OBJECT IDENTIFIER ::= { cdrIgmp 2 }

	cdrIgmpStatusTable OBJECT-TYPE
		SYNTAX SEQUENCE OF CdrIgmpStatusEntry
		MAX-ACCESS not-accessible
		STATUS current
		DESCRIPTION ""
		::= { cdrIgmpStatus 1 }

	cdrIgmpStatusEntry OBJECT-TYPE
	SYNTAX CdrIgmpStatusEntry
	MAX-ACCESS not-accessible
	STATUS current
	DESCRIPTION "IGMP CDR log"
	INDEX {
		cdrIgmpStatusEntryIndex
	}
	::= { cdrIgmpStatusTable 1 }

	CdrIgmpStatusEntry ::= SEQUENCE {
		cdrIgmpStatusEntryIndex INTEGER,
		cdrIgmpEventTime DisplayString,
		cdrIgmpEventType INTEGER,
		cdrIgmpGroup InetAddress,
		cdrIgmpPrivilege INTEGER,
		cdrIgmpUptime DisplayString,
		cdrIgmpChannel DisplayString
	}

	cdrIgmpStatusEntryIndex OBJECT-TYPE
		SYNTAX INTEGER
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "CDR IGMP table entry index."
		::= { cdrIgmpStatusEntry 1 }

	cdrIgmpEventTime OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "Time of event occurrence."
		::= { cdrIgmpStatusEntry 2 }

	cdrIgmpEventType OBJECT-TYPE
		SYNTAX INTEGER {
			leaveAutonomous(0),
			leaveTimeout(1),
			leaveEnforce(2),
			joinSuccess(3),
			joinFail(4),
			unknown(5)
		}
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "IGMP report type."
		::= { cdrIgmpStatusEntry 3 }

	cdrIgmpGroup OBJECT-TYPE
		SYNTAX InetAddress (SIZE(4|16))
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "IGMP group address."
		::= { cdrIgmpStatusEntry 4 }

	cdrIgmpPrivilege OBJECT-TYPE
		SYNTAX INTEGER {
			permit(0),
			preview(1),
			reject(2),
			unknown(3)
		}
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION ""
		::= { cdrIgmpStatusEntry 5 }

	cdrIgmpUptime OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "Group uptime."
		::= { cdrIgmpStatusEntry 6 }

	cdrIgmpChannel OBJECT-TYPE
		SYNTAX DisplayString
		MAX-ACCESS read-only
		STATUS current
		DESCRIPTION "IGMP channel."
		::= { cdrIgmpStatusEntry 7 }

