            }
        
        hwVdsl2nniPMLCurrUnit  OBJECT-TYPE
            SYNTAX      HwXdslUnit
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The termination unit XTUC{1} or XTUR{2}."
            ::= { hwVdsl2nniPMLineCurrEntry 1 }
        
        hwVdsl2nniPMLCurrValidIntervals  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Valid intervals."
            ::= { hwVdsl2nniPMLineCurrEntry 2 }
        
        hwVdsl2nniPMLCurrInvalidIntervals  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Invalid intervals."
            ::= { hwVdsl2nniPMLineCurrEntry 3 }
        
        hwVdsl2nniPMLCurr15MTimeElapsed  OBJECT-TYPE
            SYNTAX      Unsigned32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Total elapsed seconds in this interval.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMLineCurrEntry 4 }
        
        hwVdsl2nniPMLCurr15MFecs  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds during this interval that there was at least
                 one FEC correction event for one or more bearer channels in
                 this line.  This parameter is inhibited during UAS or SES.
                 Unit: seconds
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 5 }
           
        hwVdsl2nniPMLCurr15MEs  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds during this interval that there was:
                 xTU-C: CRC-8 &gt;= 1 for one or more bearer channels OR
                        LOS &gt;= 1 OR SEF &gt;=1 OR LPR &gt;= 1
                 xTU-R: FEBE &gt;= 1 for one or more bearer channels OR
                        LOS-FE &gt;=1 OR RDI &gt;=1 OR LPR-FE &gt;=1 .
                 This parameter is inhibited during UAS.
                 Unit: seconds
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 6 }
        
        hwVdsl2nniPMLCurr15MSes  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds during this interval that there was:
                 xTU-C:  If G.998.4 is supported and retransmission is enabled 
                         in the near-end received direction, a SES is declared 
                         if during a 1-second interval, there is one or more 
                         severe loss of error-free throughput (seftr) defect 
                         in the near-end received direction.
                         Or else, an SES is declared if, during a 1 second interval, 
                         there are 18 or more CRC 8 anomalies in one or more of the 
                         received bearer channels, or one or more LOS defects, or 
                         one or more SEF defects, or one or more LPR defects.
                        
                 xTU-R: If G.998.4 is supported and retransmission is enabled in the 
                        far-end received direction, a SES-LFE is declared if during 
                        a 1-second interval, there is one or more severe loss of 
                        error-free throughput (seftr) defect in the far-end received 
                        direction.
                        Or else, an SES is declared if, during a 1 second interval, 
                        there are 18 or more FEBE anomalies in one or more of the 
                        transmitted bearer channels, or one or more far end LOS 
                        defects, or one or more RDI defects, or one or more LPR FE 
                        defects.
                 This parameter is inhibited during UAS.
                 Unit: seconds
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 7 }
        
        hwVdsl2nniPMLCurr15MLoss  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds during this interval that there was LOS (or
                 LOS-FE for xTU-R).
                 Unit: seconds
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 8 }
          
        hwVdsl2nniPMLCurr15MUas  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds in Unavailability State during this
                 interval.  Unavailability begins at the onset of 10
                 contiguous severely-errored seconds, and ends at the
                 onset of 10 contiguous seconds with no severely-errored
                 seconds.
                 Unit: seconds
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 9 }
           
        hwVdsl2nniPMLCurr15MLofs  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds in the current 15 minute interval when there was Loss of Framing.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMLineCurrEntry 10 }
            
        hwVdsl2nniPMLCurr15MLols  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds in the current 15 minute interval when there was Loss of Link.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMLineCurrEntry 11 }
        
        hwVdsl2nniPMLCurr15MLops  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of seconds in the current 15 minute interval when there was Loss of Power.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMLineCurrEntry 12 }  
        
        hwVdsl2nniPMLCurr15MSosSuccess  OBJECT-TYPE
            SYNTAX      Counter32
            UNITS       "times"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of the total number of successful SOS procedures initiated in the current 15 minute.
                 Unit: times
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 13 }
            
        hwVdsl2nniPMLCurr15MInmainpeq  OBJECT-TYPE
            SYNTAX      OCTET STRING  (SIZE(0..68))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This parameter is a count of the INMAINPEQi anomalies 
                 occurring on the line during current 15 minutes.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 14 }        
            
        hwVdsl2nniPMLCurr15MInmame  OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This parameter is a count of the INMAME anomalies 
                 occurring on the line during current 15 minutes.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 15 }    
            
        hwVdsl2nniPMLCurr15MInmaiat  OBJECT-TYPE
            SYNTAX      OCTET STRING  (SIZE(0..32))
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "This parameter is a count of the INMAIATi anomalies 
                 occurring on the line during current 15 minutes.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrEntry 16 }    
           
        hwVdsl2nniPMLineCurr15MStartTime  OBJECT-TYPE
            SYNTAX      DateAndTime
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Start time of the current 15-minute performance statistics."
            ::= { hwVdsl2nniPMLineCurrEntry 17 }        
            
        hwVdsl2nniPMLCurr15MLeftrs     OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "If retransmission is used in a given transmit direction, this parameter 
                 is a count of the seconds with a near-end leftr defect present observed 
                 over the 15 minutes accumulation period.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1.1.6 7.2.1.2.6"    
            ::= { hwVdsl2nniPMLineCurrEntry 18 }
                          
        hwVdsl2nniPMLCurr15MEfb        OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "If retransmission is used in a given transmit direction, this parameter 
                 is a count of the number of error-free bits observed over the 15 minutes
                 accumulation period.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1.1.7 7.2.1.2.7"     
            ::= { hwVdsl2nniPMLineCurrEntry 19 } 
            
        hwVdsl2nniPMLCurr15MMinEftr    OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "If retransmission is used in a given transmit direction, this parameter 
                 reports the minimum of the EFTR(Error-free throughput rate) observed 
                 over the 15 minutes accumulation period.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1.1.8 7.2.1.2.8"     
            ::= { hwVdsl2nniPMLineCurrEntry 20 } 
                                           
          --          PM line init current counters     
        
        hwVdsl2nniPMLineCurrInitTable   OBJECT-TYPE
            SYNTAX      SEQUENCE  OF  HwVdsl2nniPMLineCurrInitEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The table hwVdsl2nniPMLineCurrInitTable contains current
                 initialization counters of VDSL2NNI line.
                 The index of this table is ifIndex.
                "
            ::= { hwVdsl2nniPMLine 2 }
        
        hwVdsl2nniPMLineCurrInitEntry  OBJECT-TYPE
            SYNTAX      HwVdsl2nniPMLineCurrInitEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The table hwVdsl2nniPMLineCurrInitTable contains current
                 initialization counters of VDSL2NNI line.
                 The index of this entry is ifIndex.
                "
            INDEX  { ifIndex }
            ::= { hwVdsl2nniPMLineCurrInitTable 1 }
        
        HwVdsl2nniPMLineCurrInitEntry  ::=
            SEQUENCE {
              hwVdsl2nniPMLCurrInit15MTimeElapsed          Unsigned32,
              hwVdsl2nniPMLCurrInit15MFullInits            Unsigned32,
              hwVdsl2nniPMLCurrInit15MFailedFullInits      Unsigned32,
              hwVdsl2nniPMLCurrInit15MShortInits           Unsigned32,
              hwVdsl2nniPMLCurrInit15MFailedShortInits     Unsigned32,
              hwVdsl2nniPMLineCurrInit15MStartTime         DateAndTime
            }
        
        hwVdsl2nniPMLCurrInit15MTimeElapsed  OBJECT-TYPE
            SYNTAX      Unsigned32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Total elapsed seconds in this interval.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMLineCurrInitEntry 1 }
           
        hwVdsl2nniPMLCurrInit15MFullInits  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of full initializations attempted on the line
                 (successful and failed) during this interval.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrInitEntry 2 }
        
        hwVdsl2nniPMLCurrInit15MFailedFullInits  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of failed full initializations on the line during this
                 interval.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrInitEntry 3 }
        
        hwVdsl2nniPMLCurrInit15MShortInits  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of short initializations attempted on the line
                 (successful and failed) during this interval.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrInitEntry 4 }
        
        hwVdsl2nniPMLCurrInit15MFailedShortInits  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of failed short initializations on the line during this
                 interval.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.1"
            ::= { hwVdsl2nniPMLineCurrInitEntry 5 }

        hwVdsl2nniPMLineCurrInit15MStartTime  OBJECT-TYPE
            SYNTAX      DateAndTime
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Start time of the current initializations 15-minute performance statistics."
            ::= { hwVdsl2nniPMLineCurrInitEntry 6 } 
            
        --  PM channel current counters 
                     
        hwVdsl2nniPMChCurrTable        OBJECT-TYPE
            SYNTAX      SEQUENCE  OF  HwVdsl2nniPMChCurrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The table hwVdsl2nniPMChCurrTable contains current Performance
                 Monitoring results of VDSL2NNI channel. 
                 The indexes of this table are ifIndex and hwVdsl2nniPMChCurrUnit.
                "
            ::= { hwVdsl2nniPMChannel 1 }
        
        hwVdsl2nniPMChCurrEntry  OBJECT-TYPE
            SYNTAX      HwVdsl2nniPMChCurrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The table hwVdsl2nniPMChCurrTable contains current Performance
                 Monitoring results of VDSL2NNI channel.
                 The indexes of this entry are ifIndex and hwVdsl2nniPMChCurrUnit.
                "
            INDEX  { ifIndex, hwVdsl2nniPMChCurrUnit }
            ::= { hwVdsl2nniPMChCurrTable 1 }
        
        HwVdsl2nniPMChCurrEntry  ::=
            SEQUENCE {
              hwVdsl2nniPMChCurrUnit                     HwXdslUnit,
              hwVdsl2nniPMChCurrValidIntervals           Unsigned32,
              hwVdsl2nniPMChCurrInvalidIntervals         Unsigned32,
              hwVdsl2nniPMChCurr15MTimeElapsed           Unsigned32,
              hwVdsl2nniPMChCurr15MCodingViolations      Unsigned32,
              hwVdsl2nniPMChCurr15MCorrectedBlocks       Unsigned32,
              hwVdsl2nniPMChCurr15MEncodedBlocks         Unsigned32,
              hwVdsl2nniPMChCurr15MUncorrectableBlocks   Unsigned32,
              hwVdsl2nniPMChCurr15MAtmCells              Counter32,
              hwVdsl2nniPMChCurr15MPtmPackets            Counter32,  
              hwVdsl2nniPMChCurr15MRtxCW		 Counter32,
              hwVdsl2nniPMChCurr15MRtxCorrectedCW	 Counter32,
              hwVdsl2nniPMChCurr15MRtxUncorrectedCW      Counter32,
              hwVdsl2nniPMChCurr15MStartTime             DateAndTime
            }
        
        hwVdsl2nniPMChCurrUnit  OBJECT-TYPE
            SYNTAX      HwXdslUnit
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The termination unit."
            ::= { hwVdsl2nniPMChCurrEntry 1 }
           
        hwVdsl2nniPMChCurrValidIntervals  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Valid intervals."
            ::= { hwVdsl2nniPMChCurrEntry 2 }
        
        hwVdsl2nniPMChCurrInvalidIntervals  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Invalid intervals."
            ::= { hwVdsl2nniPMChCurrEntry 3 }
        
        hwVdsl2nniPMChCurr15MTimeElapsed  OBJECT-TYPE
            SYNTAX      Unsigned32
            UNITS       "seconds"
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Total elapsed seconds in this interval.
                 Unit: seconds
                "
            ::= { hwVdsl2nniPMChCurrEntry 4 }
        
        hwVdsl2nniPMChCurr15MCodingViolations  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of CRC-8 (FEBE for xTU-R) anomalies occurring in the
                 channel during the interval.  This parameter is inhibited
                 during UAS or SES.  If the CRC is applied over multiple
                 channels, then each related CRC-8 (or FEBE) anomaly should
                 increment each of the counters related to the individual
                 channels.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.2"
            ::= { hwVdsl2nniPMChCurrEntry 5 }
        
        hwVdsl2nniPMChCurr15MCorrectedBlocks  OBJECT-TYPE
            SYNTAX      Unsigned32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of FEC (FFEC for xTU-R) anomalies (corrected code words)
                 occurring in the channel during the interval.  This parameter
                 is inhibited during UAS or SES.  If the FEC is applied over
                 multiple channels, then each related FEC (or FFEC) anomaly
                 should increment each of the counters related to the
                 individual channels.
                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.2.2"
            ::= { hwVdsl2nniPMChCurrEntry 6 }
        
        hwVdsl2nniPMChCurr15MEncodedBlocks  OBJECT-TYPE
            SYNTAX      Unsigned32         
            MAX-ACCESS  read-only          
            STATUS      current            
            DESCRIPTION                    
                "Count of all encoded blocks received 
                 on this channel within the current 15 minute interval.
                "      
            ::= { hwVdsl2nniPMChCurrEntry 7 }
            
        hwVdsl2nniPMChCurr15MUncorrectableBlocks  OBJECT-TYPE
            SYNTAX      Unsigned32         
            MAX-ACCESS  read-only          
            STATUS      current            
            DESCRIPTION                    
                "Count of all blocks received with 
                 uncorrectable errors on this channel within the current 15 minute interval.
                "      
            ::= { hwVdsl2nniPMChCurrEntry 8 }
        
        hwVdsl2nniPMChCurr15MAtmCells  OBJECT-TYPE
            SYNTAX      Counter32         
            MAX-ACCESS  read-only          
            STATUS      current            
            DESCRIPTION                    
                "Count of Atm Cells received
                 within the current 15 minute interval.
                "      
            ::= { hwVdsl2nniPMChCurrEntry 9 }
        
        hwVdsl2nniPMChCurr15MPtmPackets  OBJECT-TYPE
            SYNTAX      Counter32         
            MAX-ACCESS  read-only          
            STATUS      current            
            DESCRIPTION                    
                "Count of Ptm Packet received
                 within the current 15 minute interval.
                "      
            ::= { hwVdsl2nniPMChCurrEntry 10 }
           
        hwVdsl2nniPMChCurr15MRtxCW  OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
               "Count of units request retransmission within the current 15 minute interval."
            ::= { hwVdsl2nniPMChCurrEntry 11 }
           
        hwVdsl2nniPMChCurr15MRtxCorrectedCW  OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of units corrected by retransmission within the current 15 minute interval."
            ::= { hwVdsl2nniPMChCurrEntry 12 }
           
        hwVdsl2nniPMChCurr15MRtxUncorrectedCW  OBJECT-TYPE
            SYNTAX      Counter32
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Count of units uncorrected by retransmission within the current 15 minute interval."
            ::= { hwVdsl2nniPMChCurrEntry 13 }

        hwVdsl2nniPMChCurr15MStartTime  OBJECT-TYPE
            SYNTAX      DateAndTime
            MAX-ACCESS  read-only
            STATUS      current
            DESCRIPTION
                "Start time of the channel current 15-minute performance statistics."
            ::= { hwVdsl2nniPMChCurrEntry 14 }    

 END     