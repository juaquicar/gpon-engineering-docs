		SEQUENCE 
		{
		    hwVdsl2nniRxFrameCount  Counter32,
		    hwVdsl2nniRxBytesCount  Counter32,
		    hwVdsl2nniTxFrameCount  Counter32,
		    hwVdsl2nniTxBytesCount  Counter32,
		    hwVdsl2nniStatisticsClear INTEGER
   	        }
   	
	hwVdsl2nniRxFrameCount  OBJECT-TYPE
		SYNTAX      Counter32
		UNITS       "frames"
		MAX-ACCESS  read-only
		STATUS      current
      	        DESCRIPTION
	           "The VDSL2NNI port receive OK frames."
		::= { hwVdsl2nniStatisticsEntry 1 }
		
	hwVdsl2nniRxBytesCount  OBJECT-TYPE
		SYNTAX      Counter32
		UNITS       "bytes"
		MAX-ACCESS  read-only
		STATUS      current
      	        DESCRIPTION
	           "The VDSL2NNI port receive OK bytes."
		::= { hwVdsl2nniStatisticsEntry 2 }
		
	hwVdsl2nniTxFrameCount  OBJECT-TYPE
		SYNTAX      Counter32
		UNITS       "frames"
		MAX-ACCESS  read-only
		STATUS      current
      	        DESCRIPTION
		   "The VDSL2NNI port transmit OK frames."
		::= { hwVdsl2nniStatisticsEntry 3 }
		
	hwVdsl2nniTxBytesCount  OBJECT-TYPE
		SYNTAX      Counter32
		UNITS       "bytes"
		MAX-ACCESS  read-only
		STATUS      current
      	        DESCRIPTION
		   "The VDSL2NNI port transmit OK bytes."
		::= { hwVdsl2nniStatisticsEntry 4 }				
		
	hwVdsl2nniStatisticsClear  OBJECT-TYPE
		SYNTAX      INTEGER
		{
		    resetStat(1)
		}
		MAX-ACCESS  read-write
		STATUS      current
      	        DESCRIPTION
		   "Clear the statistics of VDSL NNI, 
		    Options:
		    1. resetStat(1)  -clear the statistics
		   "                              
		::= { hwVdsl2nniStatisticsEntry 5 }
            
          --   PM line current counters       
   
        hwVdsl2nniPMLineCurrTable  OBJECT-TYPE
            SYNTAX      SEQUENCE  OF  HwVdsl2nniPMLineCurrEntry
            MAX-ACCESS  not-accessible
            STATUS      current
            DESCRIPTION
                "The table hwVdsl2nniPMLineCurrTable contains current Performance
