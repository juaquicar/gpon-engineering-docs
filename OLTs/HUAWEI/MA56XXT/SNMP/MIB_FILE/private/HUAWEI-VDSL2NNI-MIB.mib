                "
            REFERENCE    "ITU-T G.997.1, paragraph 7.5.1.16"
            ::= { hwVdsl2nniLineEntry 18 }
        
        hwVdsl2nniLineStatusActAtpUs  OBJECT-TYPE
            SYNTAX      Integer32
            UNITS       "0.1 dB"
